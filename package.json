{
  "name": "aws-k8s",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "@types/body-parser": "^1.19.3",
    "express": "^4.18.2"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon -e ts -w ./src -x npm run dev:serve",
    "dev:serve": "ts-node ./src/index.ts",
    "build": "tsc --build tsconfig.json",
    "clean": "tsc --build --clean",
    "test": "jest ",
    "test:coverage": "yarn run test --coverage --passWithNoTests",
    "test:ci": "yarn run test:coverage --ci --runInBand",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --config .prettierrc \"**/*.{css,scss,js,jsx,ts,tsx,md}\" --write",
    "format:check": "prettier --config .prettierrc \"**/*.{js,jsx,ts,tsx,md}\" --check",
    "format:test": "prettier --config .prettierrc \"**/*.{css,scss,js,jsx,ts,tsx,md}\" -l",
    "tsc": "yarn tsc:check",
    "tsc:check": "yarn --noEmit --incremental",
    "validate": "yarn run format:check && yarn run lint && yarn run tsc:check"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-ridouku:ridouku/aws-k8s.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.86",
    "@swc/helpers": "^0.5.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "jest-standard-reporter": "^2.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
